package com.ecomm.model;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table
public class Order {

	@Id
	int orderId;
	int cartId;
	String username;
	int productId;
	String productName;
	int quantity;
	int totalAmount;
	String dateTime;
	String paid;
}